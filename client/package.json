{
  "name": "pawpals",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "framer-motion": "^10.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-outside-click-handler": "^1.3.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "storybook": "^7.0.3",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-css": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "build-css": "tailwindcss -i ./src/input.css -o ./public/output.css",
    "dev": "concurrently \"npm start\" \"npm run watch-css\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.3",
    "@storybook/addon-interactions": "^7.0.3",
    "@storybook/addon-links": "^7.0.3",
    "@storybook/addon-styling": "^1.0.0",
    "@storybook/blocks": "^7.0.3",
    "@storybook/preset-create-react-app": "^7.0.3",
    "@storybook/react": "^7.0.3",
    "@storybook/react-webpack5": "^7.0.3",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react-helmet": "^6.1.6",
    "@types/react-outside-click-handler": "^1.3.1",
    "autoprefixer": "^10.4.14",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.2.4",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.3.1",
    "webpack": "^5.78.0"
  }
}
